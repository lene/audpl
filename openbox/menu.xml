<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">


  <menu id="root-menu" label="Openbox 3">
	<item label="MRxvt">
	  <action name="Execute">
		<execute>mrxvt</execute>
	  </action>
	</item>
	<item label="Rox">
	  <action name="Execute">
		<execute>rox</execute>
	  </action>
	</item>

	<separator/>

	<menu id="apps" label="Apps">

	  <menu id="net-menu" label="Net">

		<item label="Firefox">
		  <action name="Execute">
			<execute>firefox</execute>
		  </action>
		</item>
		<item label="Thunderbird">
		  <action name="Execute">
			<execute>thunderbird</execute>
		  </action>
		</item>
		<item label="Nicotine">
		  <action name="Execute">
			<execute>/home/lene/scripts/nicotine-tor</execute>
		  </action>
		</item>
		<item label="Skype">
		  <action name="Execute">
			<execute>skype</execute>
		  </action>
		</item>

	  </menu>

	  <menu id="dev-menu" label="Dev">

		<item label="Emacs">
		  <action name="Execute">
			<execute>emacs</execute>
		  </action>
		</item>
		<item label="PyCharm">
		  <action name="Execute">
			<execute>pycharm.sh</execute>
		  </action>
		</item>
		<item label="PhpStorm">
		  <action name="Execute">
			<execute>phpstorm.sh</execute>
		  </action>
		</item>
		<item label="CLion">
		  <action name="Execute">
			<execute>/opt/clion/bin/clion.sh</execute>
		  </action>
		</item>

	  </menu>
	  
      <menu id="fun" label="Fun">

		<item label="Audacious">
		  <action name="Execute">
			<execute>audacious</execute>
		  </action>
		</item>
		<item label="FBReader">
		  <action name="Execute">
			<execute>fbreader</execute>
		  </action>
		</item>

	  </menu>
	  
	  <menu id="/Debian"/>

	</menu>
	
	<separator/>


    <menu id="config" label="Config">

	  <menu id="client-list-menu"/>

	  <item label="ObConf">
		<action name="Execute">
		  <execute>obconf</execute>
		</action>
	  </item>
	  <item label="Wallpaper">
		<action name="Execute">
		  <execute>nitrogen</execute>
		</action>
	  </item>
	  <item label="Reconfig">
		<action name="Reconfigure"/>
	  </item>
	  <item label="Restart">
		<action name="Restart"/>
	  </item>
	  <item label="Pulse Audio Volume Control">
		<action name="Execute">
		  <execute>pavucontrol</execute>
		</action>
	  </item>
	</menu>

	<separator/>
	
	<item label="Lock">
	  <action name="Execute">
		<execute>xscreensaver-command -lock</execute>
	  </action>
	</item>
	<item label="Exit">
	  <action name="Exit"/>
	</item>

  </menu>
</openbox_menu>
